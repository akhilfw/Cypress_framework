{
  "name": "cypress_framework",
  "version": "1.0.0",
  "description": "Cypress iwth Cucumber",
  "scripts": {
    "cy:open": "cypress open --env username=$USERNAME, password=$PASSWORD",
    "cy:run": "cypress run --env username=$USERNAME, password=$PASSWORD"
  },
  "keywords": [
    "Cypress",
    "BDD",
    "Cucumber"
  ],
  "license": "ISC",
  "cypress-cucumber-preprocessor": {
    "nonGlobaStepDefinitions": true,
    "step_definitions": "cypress/e2e/"
  },
  "devDependencies": {
    "cypress": "^13.15.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-dotenv": "^2.0.0",
    "dotenv": "^16.4.5"
  }
}
